function removeDuplicates(nums: number[]): number {
    if (nums.length === 0) return 0;
    
    let k = 1; // Position for next unique element (start at 1 since first element is always unique)
    
    for (let i = 1; i < nums.length; i++) {
        if (nums[i] !== nums[i - 1]) {
            nums[k] = nums[i];
            k++;
        }
    }
    
    return k;
}